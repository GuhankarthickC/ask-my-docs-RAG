{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AzureStorage": {
    "ConnectionString": "<SET_AZURE_STORAGE_CONNECTION_STRING>",
    "ContainerName": "<SET_AZURE_STORAGE_CONTAINER_NAME>"
  },
  "AzureOpenAI": {
    "Endpoint": "<SET_AZURE_OPENAI_ENDPOINT>",
    "ApiKey": "<SET_AZURE_OPENAI_KEY>"
  },
  "AzureSearch": {
    "Endpoint": "<SET_AZURE_SEARCH_ENDPOINT>",
    "IndexName": "<SET_AZURE_SEARCH_INDEX_NAME>",
    "ApiKey": "<SET_AZURE_SEARCH_ADMIN_KEY>",
    "MaxResults": 1
  }
}
